{
  "name": "Havn Daggry Avenir",
  "comment": "This is supposed to work without any extra fonts installed.",
  "style": "Light",
  "author": {
    "name": "Havn.blog",
    "email": "post@havn.online"
  },
  "editor": {
    "backgroundColor": "#fcfcfd",
    "altBackgroundColor": "#f2f3f8",
    "tintColor": "386a51",
    "tintColor2": "#9b73ab",
    "textColor": "#212840",
    "toolbarBackgroundColor": "#f2f3f8",
    "toolbarIconColor": "#3e4a77",
    "menuItemColor": "#3e4a77",
    "timeBlockColor": "#da8174",
    "sidebarStyleOverride": "light",
    "sidebarIconColorOverride": "#3e4a77",
    "shouldOverwriteFont": false
  },
  "orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "special-char",
    "checked-todo-characters",
    "checked-scheduled",
    "quote-mark",
    "NoteLinks-main",
    "link",
    "Timeblock-Hide-Tag",
    "schedule-to-date-link",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3",
    "note-title-link",
    "highlighted",
    "highlighted-left-marker",
    "highlighted-right-marker",
    "working-on",
    "flagged-1",
    "flagged-2",
    "flagged-3",
    "starred",
    "starred-list",
    "interesting-border",
    "interesting-content",
    "interesting-heading",
    "interesting-right-delimiter",
    "interesting-left-delimiter",
    "disagree-border",
    "disagree-content",
    "disagree-heading",
    "disagree-right-delimiter",
    "disagree-left-delimiter",
    "reading-border",
    "reading-content",
    "reading-heading",
    "reading-right-delimiter",
    "reading-left-delimiter",
    "definition-border",
    "definition-content",
    "definition-heading",
    "definition-right-delimiter",
    "definition-left-delimiter",
    "key-border",
    "key-content",
    "key-heading",
    "key-right-delimiter",
    "key-left-delimiter",
    "critique-border",
    "critique-content",
    "critique-heading",
    "critique-right-delimiter",
    "critique-left-delimiter",
    "example-border",
    "example-content",
    "example-heading",
    "example-right-delimiter",
    "example-left-delimiter",
    "todo",
    "phonenumber",
    "deadline",
    "hashtag",
    "attag",
    "timeblocks",
    "checked",
    "checked-canceled",
    "checked-canceled-text",
    "done-date",
    "tabbed"
  ],
  "styles": {
    "body": {
      "font": "AvenirNext-Regular",
      "size": 16,
      "color": "#3e4a77",
      "lineSpacing": 2,
      "paragraphSpacingBefore": 1,
      "paragraphSpacing": 6
    },

    "tabbed": {
      "headIndent": 43
    },

    "bold": {
      "font": "AvenirNext-DemiBold",
      "color": "#dd3e4a77"
    },
    "bold-left-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#403e4a77"
    },
    "bold-right-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#403e4a77"
    },

    "italic": {
      "font": "AvenirNext-Italic",
      "color": "#2c3454"
    },
    "italic-left-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 12,
      "color": "#40386a51"
    },
    "italic-right-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 12,
      "color": "#40386a51"
    },

    "boldItalic": {
      "font": "AvenirNext-DemiBoldItalic",
      "color": "#cc2c3454"
    },
    "boldItalic-left-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 8,
      "color": "#403e6f77"
    },
    "boldItalic-right-mark": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 8,
      "color": "#403e6f77"
    },

    "underline": {
      "underlineStyle": 1,
      "underlineColor": "#468666"
    },
    "underline-left-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 12,
      "color": "#99468666"
    },
    "underline-right-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 12,
      "color": "#99468666"
    },

    "strikethrough": {
      "strikethroughStyle": 2,
      "color": "#97a1c8",
      "strikethroughColor": "#3a4a77"
    },
    "strikethrough-left-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#97a1c8"
    },
    "strikethrough-right-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#97a1c8"
    },

    "highlighted": {
      "backgroundColor": "#20da8174",
      "color": "#30395c",
      "leftBorderPadding": 4,
      "rightBorderPadding": 4,
      "borderRadius": 6,
      "horizontalMargin": 6,
      "order": 35
    },
    "highlighted-left-marker": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#66da8174"
    },
    "highlighted-right-marker": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "size": 10,
      "color": "#66da8174"
    },

    "inline-comment": {
      "regex": "(%)(%([^%]+)%)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "color": "#a986b6"
    },
    "inline-comment-mark-start": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9b73ab"
    },
    "inline-comment-mark-end": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9b73ab"
    },

    "code": {
      "font": "Menlo-Regular",
      "size": 14,
      "backgroundColor": "#0F243B91",
      "color": "#30395c",
      "leftBorderPadding": 8,
      "rightBorderPadding": 6,
      "inlineBorder": false,
      "borderRadius": 6,
      "horizontalMargin": 2
    },
    "code-left-backtick": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "color": "#97a1c8"
    },
    "code-right-backtick": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "color": "#97a1c8"
    },

    "code-fence": {
      "theme": "atom-one-light",
      "body": {
        "font": "Menlo-Regular",
        "size": 14,
        "backgroundColor": "#0F243B91",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0
      },
      "corner-radius": 8,
      "fence-open": {
        "color": "#97a1c8"
      },
      "fence-close": {
        "isMarkdownCharacter": true,
        "isHiddenWithoutCursor": false,
        "color": "#97a1c8"
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "size": 14,
        "color": "#97a1c8",
        "paragraphSpacing": 0
      }
    },

    "quote-mark": {
      "headIndent": 33,
      "size": 16,
      "color": "#386a51",
      "font": "noteplanstate"
    },
    "quote-content": {
      "headIndent": 33,
      "color": "#363f66",
      "lineSpacing": 0.9,
      "paragraphSpacing": 4,
      "paragraphSpacingBefore": 0
    },

    "title1": {
      "font": "AvenirNext-Bold",
      "size": 23,
      "color": "#212840",
      "_horizontalMargin": -6
    },
    "title-in-frontmatter": {
      "regex": "^[tT]itle:\\s+(.*)",
      "matchPosition": 1,
      "font": "AvenirNext-Medium",
      "size": 16,
      "color": "#212840"
    },
    "title-mark1": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "font": "AvenirNext-UltraLight",
      "size": 23,
      "color": "#50212840",
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 9
    },

    "title2": {
      "font": "AvenirNext-DemiBold",
      "size": 22,
      "color": "#29304e",
      "_horizontalMargin": -5
    },
    "title-mark2": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "font": "AvenirNext-UltraLight",
      "size": 22,
      "color": "#5029304e",
      "paragraphSpacingBefore": 16,
      "paragraphSpacing": 6
    },

    "title3": {
      "font": "AvenirNext-Medium",
      "size": 21,
      "color": "#30395c",
      "_horizontalMargin": -4
    },
    "title-mark3": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "font": "AvenirNext-UltraLight",
      "size": 21,
      "color": "#5030395c",
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 6
    },

    "title4": {
      "font": "AvenirNext-Regular",
      "size": 20,
      "color": "#363f66",
      "_horizontalMargin": -3
    },
    "title-mark4": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "font": "AvenirNext-UltraLight",
      "size": 20,
      "color": "#50363f66",
      "paragraphSpacingBefore": 12,
      "paragraphSpacing": 6
    },

    "Templates_tags-opener": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 1,
      "font": "Menlo-Regular",
      "size": 14,
      "color": "#809b73ab"
    },
    "Templates_tags": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 2,
      "font": "Menlo-Regular",
      "size": 14,
      "color": "#a986b6"
    },
    "Templates_tags-closer": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 3,
      "font": "Menlo-Regular",
      "size": 14,
      "color": "#809b73ab"
    },
    "Templates_tags-commented-out": {
      "regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
      "matchPosition": 1,
      "font": "Menlo-Regular",
      "size": 14,
      "color": "#779b73ab"
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "todo": {
      "COMMENT": "Covers todos and bullets",
      "font": "noteplanstate",
      "color": "#386a51",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked": {
      "color": "#97a1c8",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
      "matchPosition": 0,
      "color": "#e94f2d",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled": {
      "COMMENT": "Covers whole line, colouring it",
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
      "color": "#80e8b017",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled-text": {
      "COMMENT": "Strikes out just the text",
      "regex": "(?:(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ))(.*)",
      "matchPosition": 3,
      "color": "#80e8b017",
      "strikethroughColor": "#50e8b017",
      "strikethroughStyle": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "done-date": {
      "color": "#97a1c8",
      "backgroundColor": "#40f2f3f8",
      "leftBorderPadding": 5,
      "rightBorderPadding": 5,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "size": 14
    },

    "working-on": {
      "backgroundColor": "#20468666",
      "color": "#30395c",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "flagged-1": {
      "backgroundColor": "#30e9745a",
      "color": "#30395c",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },
    "flagged-2": {
      "backgroundColor": "#40e96143",
      "color": "#29304e",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },
    "flagged-3": {
      "backgroundColor": "#50e94f2d",
      "color": "#212840",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },

    "timeblocks": {
      "regex": "(?:^\\s*(?:\\-(?!\\h+\\[[\\-\\>]\\]\\s+)|[\\d+]\\.|\\#{1,5}\\s+))(?:\\[\\s\\]\\s+)?.*?\\s(((at|from)\\s+([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s?(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?))?|([0-2]?\\d|noon|midnight)(:[0-5]\\d)\\s*(AM|am|PM|pm)?(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?)?)?))(?=\\s|$)",
      "matchPosition": 1,
      "color": "#da8174"
    },

    "deadline": {
      "regex": "\\h(!\\d{4}-\\d{2}-\\d{2})\\b",
      "matchPosition": 1,
      "font": "AvenirNext-Medium",
      "color": "#e94f2d"
    },

    "hashtag": {
      "color": "#52629e",
      "backgroundColor": "#1052629e",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "type": ""
    },
    "attag": {
      "color": "#9b73ab",
      "backgroundColor": "#10a986b6",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "type": ""
    },

    "special-char": {
      "COMMENT": "Is this needed?",
      "font": "Menlo-Bold"
    },

    "phonenumber": {
      "COMMENT": "How do I render? Don't know if it works.",
      "color": "#9b73ab"
    },

    "NoteLinks-main": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 2,
      "color": "#386a51"
    },
    "note-title-link-left-bracket": {
      "color": "#8052629e",
      "isHiddenWithoutCursor": false
    },
    "note-title-link-right-bracket": {
      "color": "#8052629e",
      "isHiddenWithoutCursor": false
    },

    "URL-link-left-square-bracket": {
      "COMMENT": "Slightly more complex than the others to avoid problem with start-of-line task markers",
      "regex": "(\\[)([^]]*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 1,
      "color": " #8052629e",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "URL-link-right-square-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 3,
      "color": " #8052629e",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "URL-link-left-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 4,
      "color": " #8052629e",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "URL-link-right-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 6,
      "color": " #8052629e",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "arrow-link": {
      "regex": "[\\s^]>([^>]*?)<[$\\s\\b\\.\\)\\]\\}]",
      "matchPosition": 1,
      "urlPosition": 1,
      "backgroundColor": "#10468666",
      "leftBorderPadding": 10,
      "rightBorderPadding": 9,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "isRevealOnCursorRange": true,
      "type": "noteLink",
      "prefix": "noteplan://x-callback-url/openNote?noteTitle="
    },
    "arrow-left-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 1,
      "color": "#8052629e",
      "size": 12,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "arrow-right-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 3,
      "color": "#8052629e",
      "size": 12,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    }
  }
}
