{
  "name": "Havn Skumring San Francisco",
  "comment": "This is supposed to work without any extra fonts installed.",
  "style": "Dark",
  "author": {
    "name": "Havn.blog",
    "email": "post@havn.online"
  },
  "editor": {
    "backgroundColor": "#1a1e30",
    "altBackgroundColor": "#121521",
    "tintColor": "#54a094",
    "tintColor2": "#b699c2",
    "textColor": "#eef0f6",
    "toolbarBackgroundColor": "#121521",
    "toolbarIconColor": "#d7dbea",
    "menuItemColor": "#d7dbea",
    "timeBlockColor": "#da8174",
    "sidebarStyleOverride": "dark",
    "sidebarIconColorOverride": "#d7dbea",
    "shouldOverwriteFont": false
  },
  "orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "special-char",
    "checked-todo-characters",
    "checked-scheduled",
    "quote-mark",
    "tabbed",
    "NoteLinks-main",
    "link",
    "Timeblock-Hide-Tag",
    "schedule-to-date-link",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3",
    "note-title-link",
    "highlighted",
    "highlighted-left-marker",
    "highlighted-right-marker",
    "starred",
    "flagged-1",
    "flagged-2",
    "flagged-3",
    "todo",
    "working-on",
    "deadline",
    "hashtag",
    "attag",
    "timeblocks",
    "checked",
    "checked-canceled",
    "checked-canceled-text",
    "done-date"
  ],
  "styles": {
    "body": {
      "_font": "SFPro-Regular",
      "font": ".AppleSystemUIFont",
      "size": 16,
      "color": "#d7dbea",
      "lineSpacing": 2,
      "paragraphSpacingBefore": 1,
      "paragraphSpacing": 6
    },

    "tabbed": {
      "COMMENT": "for anything indented",
      "headIndent": 43
    },

    "bold": {
      "_font": "SFPro-Semibold",
      "font": ".AppleSystemUIFontBold"
    },
    "bold-left-mark": {
      "color": "#40d7dbea",
      "size": 12
    },
    "bold-right-mark": {
      "color": "#40d7dbea",
      "size": 12
    },

    "italic": {
      "_font": "SFPro-RegularItalic",
      "font": ".AppleSystemUIFontItalic"
    },
    "italic-left-mark": {
      "color": "#40d7dbea",
      "size": 12
    },
    "italic-right-mark": {
      "color": "#40d7dbea",
      "size": 12
    },

    "boldItalic": {
      "COMMENT": "I haven't found a way to make this work, as there no such thing as .AppleSystemUIFontBoldItalic",
      "font": ".AppleSystemUIFontBold"
    },
    "boldItalic-left-mark": {
      "color": "#40d7dbea",
      "size": 12
    },
    "boldItalic-right-mark": {
      "color": "#40d7dbea",
      "size": 12
    },

    "underline": {
      "underlineStyle": 1,
      "underlineColor": "#40786f"
    },
    "underline-left-tilde": {
      "color": "#40786f",
      "size": 12
    },
    "underline-right-tilde": {
      "color": "#40786f",
      "size": 12
    },

    "strikethrough": {
      "__regex": "(~~)([^~]+)(~~)",
      "__matchPosition": 2,
      "strikethroughStyle": 2,
      "isRevealOnCursorRange": true,
      "color": "#97a1c8",
      "strikethroughColor": "#aad7dbea"
    },
    "strikethrough-left-tilde": {
      "__regex": "(~~)([^~]+)(~~)",
      "__matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "color": "#97a1c8",
      "size": 12,
      "isRevealOnCursorRange": true
    },
    "strikethrough-right-tilde": {
      "__regex": "(~~)([^~]+)(~~)",
      "__matchPosition": 3,
      "isMarkdownCharacter": true,
      "color": "#97a1c8",
      "size": 12,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },

    "highlighted": {
      "__regex": "(==)([^\\s].+?)(==)",
      "backgroundColor": "#40da8174",
      "color": "#e5e7f1",
      "leftBorderPadding": 6,
      "rightBorderPadding": 6,
      "borderRadius": 6,
      "horizontalMargin": 6,
      "order": 35,
      "__matchPosition": 2,
      "isRevealOnCursorRange": true
    },
    "highlighted-left-marker": {
      "__regex": "(==)([^\\s].+?)(==)",
      "color": "#99da8174",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "__matchPosition": 1
    },
    "highlighted-right-marker": {
      "__regex": "(==)([^\\s].+?)(==)",
      "color": "#99da8174",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "__matchPosition": 3
    },

    "inline-comment": {
      "regex": "(%)(%([^%]+)%)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "color": "#b699c2"
    },
    "inline-comment-mark-start": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9f79af"
    },
    "inline-comment-mark-end": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9f79af"
    },

    "code": {
      "_font": "SFMono-Regular",
      "font": "Menlo-Regular",
      "color": "#e5e7f1",
      "backgroundColor": "#121521",
      "size": 14,
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2
    },
    "code-left-backtick": {
      "color": "#97a1c8"
    },
    "code-right-backtick": {
      "color": "#97a1c8"
    },

    "code-fence": {
      "theme": "atelier-plateau-dark",
      "body": {
        "_font": "SFMono-Regular",
        "font": "Menlo-Regular",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0,
        "size": 14,
        "backgroundColor": "#121521"
      },
      "corner-radius": 8,
      "fence-open": {
        "color": "#97a1c8"
      },
      "fence-close": {
        "color": "#97a1c8"
      },
      "language-specifier": {
        "_font": "SFMono-RegularItalic",
        "font": "Menlo-Italic",
        "color": "#97a1c8",
        "size": 14,
        "paragraphSpacing": 0
      }
    },

    "quote-mark": {
      "color": "#40786f",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "quote-content": {
      "color": "#e5e7f1",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4,
      "paragraphSpacingBefore": 0
    },

    "title1": {
      "__font": "NewYork-Semibold",
      "_font": "SFPro-Bold",
      "font": ".AppleSystemUIFontHeavy",
      "color": "#eef0f6",
      "size": 26
    },
    "title-in-frontmatter": {
      "regex": "^[tT]itle:\\s+(.*)",
      "matchPosition": 1,
      "__font": "NewYork-Medium",
      "_font": "SFPro-Medium",
      "font": ".AppleSystemUIFontMedium",
      "color": "#eef0f6",
      "size": 16
    },
    "title-mark1": {
      "color": "#50eef0f6",
      "size": 20,
      "font": "NewYork-Regular",
      "_font": "SFPro-Light",
      "__font": ".AppleSystemUIFontLight",
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 9
    },

    "title2": {
      "__font": "NewYork-Semibold",
      "_font": "SFPro-Semibold",
      "font": ".AppleSystemUIFontBold",
      "color": "#eaecf4",
      "size": 24
    },
    "title-mark2": {
      "color": "#50eaecf4",
      "size": 18,
      "__font": "NewYork-Regular",
      "_font": "SFPro-Light",
      "font": ".AppleSystemUIFontLight",
      "paragraphSpacingBefore": 16,
      "paragraphSpacing": 6
    },

    "title3": {
      "__font": "NewYork-Medium",
      "_font": "SFPro-Semibold",
      "font": ".AppleSystemUIFontBold",
      "color": "#e5e7f1",
      "size": 20
    },
    "title-mark3": {
      "color": "#50e5e7f1",
      "size": 14,
      "__font": "NewYork-Regular",
      "_font": "SFPro-Light",
      "font": ".AppleSystemUIFontLight",
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 6
    },

    "title4": {
      "color": "#dde0ed",
      "size": 18,
      "__font": "NewYork-Regular",
      "_font": "SFPro-Medium",
      "font": ".AppleSystemUIFontMedium"
    },
    "title-mark4": {
      "color": "#50dde0ed",
      "size": 14,
      "__font": "NewYork-Regular",
      "_font": "SFPro-Light",
      "font": ".AppleSystemUIFontLight",
      "paragraphSpacingBefore": 12,
      "paragraphSpacing": 6
    },

    "attribute": {
      "regex": "^([^:]{3,})(::)",
      "color": "#da8174",
      "matchPosition": 1
    },
    "attribute-right-colons": {
      "regex": "^([^:]{3,})(::)",
      "color": "#da8174",
      "_font": "SFPro-Medium",
      "font": ".AppleSystemUIFontMedium",
      "matchPosition": 2,
      "isMarkdownCharacter": true
    },

    "Templates_tags-opener": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#809f79af",
      "_font": "SFMono-Regular",
      "font": "Menlo-Regular",
      "size": 14
    },
    "Templates_tags": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 2,
      "color": "#9f79af",
      "_font": "SFMono-Regular",
      "font": "Menlo-Regular",
      "size": 14
    },
    "Templates_tags-closer": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 3,
      "color": "#809f79af",
      "_font": "SFMono-Regular",
      "font": "Menlo-Regular",
      "size": 14
    },
    "Templates_tags-commented-out": {
      "regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#779f79af",
      "_font": "SFMono-Regular",
      "font": "Menlo-Regular",
      "size": 14
    },

    "field-key": {
      "COMMENT": "Simplified form of YAML field identification, limited to 25 chars in field name at start of line",
      "regex": "^([^\\h#:\\-\\+\\*][\\s\\w]{3,26})(:\\h+)(?=[\\w\\d@!#]+)",
      "matchPosition": 1,
      "color": "#97a1c8",
      "lineSpacing": 0.8,
      "paragraphSpacing": 0,
      "paragraphSpacingBefore": 0
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "todo": {
      "COMMENT": "Covers todos and bullets",
      "font": "noteplanstate",
      "color": "#40786f",
      "backgroundColor": "1a1e30",
      "rightBorderPadding": 0,
      "leftBorderPadding": 0,
      "borderRadius": 0.1,
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked": {
      "color": "#52629e",
      "backgroundColor": "1a1e30",
      "borderRadius": 1,
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
      "matchPosition": 0,
      "color": "#e94f2d",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled": {
      "COMMENT": "Covers whole line, colouring it",
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
      "color": "#80e8b017",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled-text": {
      "COMMENT": "Strikes out just the text",
      "regex": "(?:(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ))(.*)",
      "matchPosition": 3,
      "color": "#80e8b017",
      "strikethroughColor": "#99e8b017",
      "strikethroughStyle": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "done-date": {
      "color": "#52629e",
      "backgroundColor": "#090b11",
      "leftBorderPadding": 5,
      "rightBorderPadding": 5,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "size": 14
    },

    "working-on": {
      "backgroundColor": "#5040786f",
      "color": "#e5e7f1",
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "starred": {
      "regex": "(^|\\v)(.*[⭐⭐️💫⭐︎].*)($|\\v)",
      "matchPosition": 2,
      "backgroundColor": "#bb74580c",
      "color": "e5e7f1",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "flagged-1": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{1}\\h.*)|(.*\\h+\\!{1}\\h.*)|(.*\\h\\!{1}))$",
      "__matchPosition": 2,
      "backgroundColor": "#30e9745a",
      "color": "#e5e7f1",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },
    "flagged-2": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{2}\\h.*)|(.*\\h+\\!{2}\\h.*)|(.*\\h\\!{2}))$",
      "__matchPosition": 2,
      "backgroundColor": "#40e96143",
      "color": "#eaecf4",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },
    "flagged-3": {
      "__regex": "^\\h*[\\*\\-\\+](\\h+(?!\\[[x-]\\])|\\[ ]\\h+|\\[>]\\h+)((\\!{3}\\h.*)|(.*\\h+\\!{3}\\h.*)|(.*\\h\\!{3}))$",
      "__matchPosition": 2,
      "backgroundColor": "#50e94f2d",
      "color": "#eef0f6",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
    },

    "timeblocks": {
      "regex": "(?:^\\s*(?:\\-(?!\\h+\\[[\\-\\>]\\]\\s+)|[\\d+]\\.|\\#{1,5}\\s+))(?:\\[\\s\\]\\s+)?.*?\\s(((at|from)\\s+([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s?(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?))?|([0-2]?\\d|noon|midnight)(:[0-5]\\d)\\s*(AM|am|PM|pm)?(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?)?)?))(?=\\s|$)",
      "matchPosition": 1,
      "color": "#da8174"
    },

    "deadline": {
      "regex": "\\h(!\\d{4}-\\d{2}-\\d{2})\\b",
      "matchPosition": 1,
      "_font": "SFPro-Semibold",
      "font": ".AppleSystemUIFontBold",
      "color": "#ed7054"
    },

    "unfinished": {
      "regex": "\\?\\?\\?",
      "color": "#1a1e30",
      "_font": "SFPro-Light",
      "font": ".AppleSystemUIFontThin",
      "backgroundColor": "#40786f",
      "leftBorderPadding": 6,
      "rightBorderPadding": 6,
      "borderRadius": 6,
      "horizontalMargin": 7
    },

    "hashtag": {
      "color": "#97a1c8",
      "backgroundColor": "#2052629e",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "type": ""
    },
    "attag": {
      "color": "#b699c2",
      "backgroundColor": "#209f79af",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "type": ""
    },

    "special-char": {
      "COMMENT": "Is this needed?",
      "_font": "SFMono-Semibold",
      "font": "Menlo-Bold"
    },

    "markdown-link": {
      "COMMENT": "Doesn't work? Pink to test.",
      "color": "#EB57FF"
    },
    "markdown-link-url": {
      "COMMENT": "Doesn't work? Blue to test.",
      "color": "#5764FF"
    },
    "link": {
      "COMMENT": "Doesn't work? Green to test.",
      "color": "#57FF84"
    },
    "time": {
      "COMMENT": "Doesn't work? Yellow/Green to test.",
      "color": "#B3FF57"
    },
    "schedule-to-date-link": {
      "COMMENT": "Doesn't work? Light brown to test.",
      "color": "#E8AE9B"
    },
    "schedule-from-date-link": {
      "COMMENT": "Doesn't work? Light brown to test.",
      "color": "#E8AE9B"
    },
    "note-title-link": {
      "COMMENT": "Doesn't work? Light orange to test.",
      "color": "#FF8D57"
    },
    "phonenumber": {
      "COMMENT": "How do I render? Don't know if it works.",
      "color": "#769EB3"
    },

    "NoteLinks-main": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 2,
      "color": "#386a51"
    },
    "NoteLinks-left-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 1,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-right-brackets": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 3,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "NoteLinks-main-done": {
      "COMMENT": "Turned off",
      "_regex": "(?:^|\\h)(x\\[\\[)(.*?)(\\]\\])",
      "_matchPosition": 1,
      "_isMarkdownCharacter": true,
      "_isRevealOnCursorRange": true,
      "_font": "noteplanstate",
      "_color": "#97B853"
    },
    "NoteLinks-main-cancelled": {
      "COMMENT": "Turned off",
      "_regex": "(?:^|\\h)(-\\[\\[)(.*?)(\\]\\])",
      "_matchPosition": 1,
      "_isMarkdownCharacter": true,
      "_isRevealOnCursorRange": true,
      "_font": "noteplanstate",
      "_color": "#C5487A"
    },
    "URL-link": {
      "_regex": "\\[[^]]*?\\]\\(.*?\\)",
      "_isRevealOnCursorRange": true,
      "COMMENT": "Turned off. Put cursor anywhere in markdown link and the []() syntax will be revealed. The following 4 do the hiding the rest of the time."
    },
    "URL-link-left-square-bracket": {
      "COMMENT": "Slightly more complex than the others to avoid problem with start-of-line task markers",
      "regex": "(\\[)([^]]*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 1,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "URL-link-right-square-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 3,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "URL-link-left-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 4,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "URL-link-right-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 6,
      "color": " #8097a1c8",
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },

    "#waiting": {
      "COMMENT": "Turned off.",
      "_regex": "^* (.*#waiting.*)$",
      "_matchPosition": 1,
      "_textColor": "#222E3388",
      "_font": "PPNeueMontreal-Italic"
    },
    "eventID": {
      "COMMENT": "Turned off.",
      "_regex": "(event:[A-F0-9-]{36,37})",
      "_matchPosition": 1,
      "_color": "#CCA949",
      "_isHiddenWithoutCursor": true,
      "_isRevealOnCursorRange": false
    },

    "arrow-link": {
      "regex": "[\\s^]>([^>]*?)<[$\\s\\b\\.\\)\\]\\}]",
      "matchPosition": 1,
      "urlPosition": 1,
      "backgroundColor": "#10468666",
      "leftBorderPadding": 10,
      "rightBorderPadding": 9,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "isRevealOnCursorRange": true,
      "type": "noteLink",
      "prefix": "noteplan://x-callback-url/openNote?noteTitle="
    },
    "arrow-left-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 1,
      "color": "#8052629e",
      "size": 12,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "arrow-right-delimiter": {
      "regex": "(>)(\\S[^<]*)(<)",
      "matchPosition": 3,
      "color": "#8052629e",
      "size": 12,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true
    },
    "verse-indicator": {
      "COMMENT": "Turned off.",
      "_regex": "(§)(.*)",
      "_matchPosition": 1,
      "_isHiddenWithoutCursor": true,
      "_isRevealOnCursorRange": true,
      "_isMarkdownCharacter": true,
