      "color": "#7a5800",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "leadingBorder": true,
      "isFullWidthBorder": true
    },
    "interesting-content": {
      "regex": "(^|\\v)(.*💡 Interesting:.*)($|\\v)",
      "backgroundColor": "#11ffb908",
      "color": "#30395c",
      "firstLineHeadIndent": 33,
      "headIndent": 33,
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4,
      "isFullWidthBorder": true
    },

    "disagree-heading": {
      "regex": "(\\>\\[!)(🚫 Disagree: )(\\])(.*)",
      "matchPosition": 2,
      "color": "#cc3333",
      "font": "PPWriter-Semibold",
      "isRevealOnCursorRange": true
    },
    "disagree-left-delimiter": {
      "regex": "(\\>\\[!)(🚫 Disagree: )(\\])(.*)",
      "matchPosition": 1,
      "color": "#77cc3333",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "disagree-right-delimiter": {
      "regex": "(\\>\\[!)(🚫 Disagree: )(\\])(.*)",
      "matchPosition": 3,
