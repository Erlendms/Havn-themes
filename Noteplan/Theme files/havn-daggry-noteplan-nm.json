  "styles": {
    "body": {
      "font": "PPNeueMontreal-Regular",
      "size": 16,
      "color": "#3e4a77",
      "lineSpacing": 2,
      "paragraphSpacingBefore": 1,
      "paragraphSpacing": 6
    },

    "tabbed": {
      "headIndent": 43
    },

    "bold": {
      "color": "#5a6796",
      "font": "PPNeueMontreal-Bold"
    },
    "bold-left-mark": {
      "color": "#97a1c8",
      "size": 10,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "bold-right-mark": {
      "color": "#97a1c8",
      "size": 10,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "italic": {
      "color": "#30395c",
      "_font": "SFPro-RegularItalic",
      "font": "PPNeueMontreal-Italic"
    },
    "italic-left-mark": {
      "color": "#97a1c8",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "italic-right-mark": {
      "color": "#97a1c8",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "boldItalic": {
      "color": "#525e89",
      "font": "PPNeueMontreal-BoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#97a1c8",
      "size": 8,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "boldItalic-right-mark": {
      "color": "#97a1c8",
      "size": 8,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#468666"
    },
    "underline-left-tilde": {
      "color": "#99468666",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "underline-right-tilde": {
      "color": "#99468666",
      "size": 12,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "parentheses": {
      "regex": "(?:^|\\s)(\\()([^)]+?)(\\))",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "color": "#5a6796"
    },
    "parentheses-left": {
      "regex": "(?:^|\\s)(\\()([^)]+?)(\\))",
      "matchPosition": 1,
      "isHiddenWithoutCursor": false,
      "color": "#363f66"
    },
    "parentheses-right": {
      "regex": "(?:^|\\s)(\\()([^)]+?)(\\))",
      "matchPosition": 3,
      "color": "#363f66",
      "isHiddenWithoutCursor": false
    },

    "strikethrough": {
      "strikethroughStyle": 2,
      "isRevealOnCursorRange": true,
      "color": "#97a1c8",
      "strikethroughColor": "#3a4a77"
    },
    "strikethrough-left-tilde": {
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false,
      "color": "#97a1c8",
      "size": 10
    },
    "strikethrough-right-tilde": {
      "isMarkdownCharacter": true,
      "color": "#97a1c8",
      "size": 10,
      "isHiddenWithoutCursor": false
    },

    "highlighted": {
      "backgroundColor": "#20da8174",
      "color": "#30395c",
      "leftBorderPadding": 4,
      "rightBorderPadding": 4,
      "borderRadius": 6,
      "horizontalMargin": 6,
      "order": 35,
      "isRevealOnCursorRange": true
    },
    "highlighted-left-marker": {
      "color": "#66da8174",
      "size": 10,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "highlighted-right-marker": {
      "color": "#66da8174",
      "size": 10,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "inline-comment": {
      "regex": "(%)(%([^%]+)%)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "color": "#a986b6"
    },
    "inline-comment-mark-start": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9b73ab"
    },
    "inline-comment-mark-end": {
      "regex": "(%%)",
      "matchPosition": 0,
      "isMarkdownCharacter": true,
      "color": "#9b73ab"
    },

    "code": {
      "font": "PPNeueMontrealMono-Book",
      "color": "#30395c",
      "backgroundColor": "#0F243B91",
      "size": 14,
      "leftBorderPadding": 8,
      "rightBorderPadding": 6,
      "inlineBorder": false,
      "borderRadius": 6,
      "horizontalMargin": 2
    },
    "code-left-backtick": {
      "color": "#97a1c8",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },
    "code-right-backtick": {
      "color": "#97a1c8",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "code-fence": {
      "theme": "atelier-dune-light",
      "body": {
        "font": "PPNeueMontrealMono-Medium",
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 0,
        "size": 14,
        "backgroundColor": "#0F243B91"
      },
      "corner-radius": 8,
      "fence-open": {
        "color": "#97a1c8",
        "isMarkdownCharacter": true,
        "isHiddenWithoutCursor": false
      },
      "fence-close": {
        "color": "#97a1c8",
        "isMarkdownCharacter": true,
        "isHiddenWithoutCursor": false
      },
      "language-specifier": {
        "font": "PPNeueMontrealMono-RegularItalic",
        "color": "#97a1c8",
        "size": 14,
        "paragraphSpacing": 0
      }
    },

    "quote-mark": {
      "color": "#386a51",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },
    "quote-content": {
      "color": "#363f66",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4,
      "paragraphSpacingBefore": 0
    },

    "title1": {
      "font": "PPWriter-Semibold",
      "color": "#212840",
      "size": 26
    },
    "title-in-frontmatter": {
      "regex": "^[tT]itle:\\s+(.*)",
      "matchPosition": 1,
      "font": "PPWriter-Medium",
      "color": "#212840",
      "size": 16
    },
    "title-mark1": {
      "color": "#97a1c8",
      "size": 16,
      "font": "PPWriter-ThinText",
      "paragraphSpacingBefore": 18,
      "paragraphSpacing": 9,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "title2": {
      "font": "PPWriter-Semibold",
      "color": "#29304e",
      "size": 24
    },
    "title-mark2": {
      "color": "#97a1c8",
      "size": 14,
      "font": "PPWriter-ThinText",
      "paragraphSpacingBefore": 16,
      "paragraphSpacing": 6,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "title3": {
      "font": "PPWriter-Medium",
      "color": "#30395c",
      "size": 20
    },
    "title-mark3": {
      "color": "#97a1c8",
      "size": 12,
      "font": "PPWriter-ThinText",
      "paragraphSpacingBefore": 14,
      "paragraphSpacing": 6,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "title4": {
      "color": "#363f66",
      "size": 18,
      "font": "PPWriter-Regular"
    },
    "title-mark4": {
      "color": "#97a1c8",
      "size": 10,
      "font": "PPWriter-ThinText",
      "paragraphSpacingBefore": 12,
      "paragraphSpacing": 6,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": false
    },

    "attribute": {
      "regex": "^([^:]{3,})(::)",
      "color": "#da8174",
      "matchPosition": 1
    },
    "attribute-right-colons": {
      "regex": "^([^:]{3,})(::)",
      "color": "#da8174",
      "font": "PPNeueMontreal-Medium",
      "matchPosition": 2,
      "isMarkdownCharacter": true
    },

    "Templates_tags-opener": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#809b73ab",
      "font": "PPNeueMontrealMono-Medium",
      "size": 14
    },
    "Templates_tags": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 2,
      "color": "#a986b6",
      "font": "PPNeueMontrealMono-Medium",
      "size": 14
    },
    "Templates_tags-closer": {
      "regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
      "matchPosition": 3,
      "color": "#809b73ab",
      "font": "PPNeueMontrealMono-Medium",
      "size": 14
    },
    "Templates_tags-commented-out": {
      "regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
      "matchPosition": 1,
      "color": "#779b73ab",
      "font": "PPNeueMontrealMono-Medium",
      "size": 14
    },

    "field-key": {
      "COMMENT": "Simplified form of YAML field identification, limited to 25 chars in field name at start of line",
      "regex": "^([^\\h#:\\-\\+\\*][\\s\\w]{3,26})(:\\h+)(?=[\\w\\d@!#]+)",
      "matchPosition": 1,
      "color": "#52629e",
      "lineSpacing": 0.8,
      "paragraphSpacing": 0,
      "paragraphSpacingBefore": 0
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "todo": {
      "COMMENT": "Covers todos and bullets",
      "font": "noteplanstate",
      "color": "#386a51",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked": {
      "color": "#97a1c8",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] ).*",
      "matchPosition": 0,
      "color": "#e94f2d",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled": {
      "COMMENT": "Covers whole line, colouring it",
      "regex": "(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ).*",
      "color": "#80e8b017",
      "headIndent": 33,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "checked-canceled-text": {
      "COMMENT": "Strikes out just the text",
      "regex": "(?:(^\\h*[\\*\\-\\+] |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] ))(.*)",
      "matchPosition": 3,
      "color": "#80e8b017",
      "strikethroughColor": "#50e8b017",
      "strikethroughStyle": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "done-date": {
      "color": "#97a1c8",
      "backgroundColor": "#40f2f3f8",
      "leftBorderPadding": 5,
      "rightBorderPadding": 5,
      "borderRadius": 6,
      "horizontalMargin": 2,
      "size": 14
    },

    "working-on": {
      "backgroundColor": "#20468666",
      "color": "#30395c",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "starred": {
      "regex": "^(?!\\* |\\+ |- |\\d+\\. ).*(⭐|⭐️|💫|⭐︎)+.*",
      "backgroundColor": "#4cffb908",
      "color": "#30395c",
      "leftBorderPadding": 3,
      "rightBorderPadding": 3,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },
    "starred-list": {
      "regex": "^(\\* |\\+ |- |\\d+\\. ).*(⭐|⭐️|💫|⭐︎)+.*",
      "backgroundColor": "#4cffb908",
      "color": "#30395c",
      "leftBorderPadding": -19,
      "rightBorderPadding": 3,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 4
    },

    "flagged-1": {
      "backgroundColor": "#30e9745a",
      "color": "#30395c",
      "leftBorderPadding": -19,
      "rightBorderPadding": 6,
      "borderRadius": 3,
      "horizontalMargin": 2,
      "lineSpacing": 0.9,
      "paragraphSpacing": 3
