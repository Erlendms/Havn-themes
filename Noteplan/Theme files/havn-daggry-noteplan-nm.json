    "key-border",
    "key-content",
    "key-heading",
    "key-right-delimiter",
    "key-left-delimiter",
    "critique-border",
    "critique-content",
    "critique-heading",
    "critique-right-delimiter",
    "critique-left-delimiter",
    "example-border",
