    "URL-link": {
      "_regex": "\\[[^]]*?\\]\\(.*?\\)",
      "_isRevealOnCursorRange": true,
      "COMMENT": "Turned off. Put cursor anywhere in markdown link and the []() syntax will be revealed. The following 4 do the hiding the rest of the time."
    },
    "URL-link-left-square-bracket": {
      "COMMENT": "Slightly more complex than the others to avoid problem with start-of-line task markers",
      "regex": "(\\[)([^]]*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 1,
      "color": "#97a1c8",
      "_isMarkdownCharacter": true
    },
    "URL-link-right-square-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 3,
      "color": "#97a1c8",
      "_isMarkdownCharacter": true
    },
    "URL-link-left-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 4,
      "color": "#97a1c8",
      "_isMarkdownCharacter": true
    },
    "URL-link-right-bracket": {
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "matchPosition": 6,
      "color": "#97a1c8",
      "_isMarkdownCharacter": true
    },

    "NoteLinks-main": {
      "regex": "(?:^|\\h)(\\[\\[)(.*?)(\\]\\])",
      "matchPosition": 2,
      "color": "#a986b6"
    },
    "note-title-link-left-bracket": {
      "color": "#97a1c8",
      "isHiddenWithoutCursor": false
    },
    "note-title-link-right-bracket": {
      "color": "#97a1c8",
      "isHiddenWithoutCursor": false
    },

    "interesting-heading": {
      "regex": "(\\>\\[!)(💡 Interesting: )(\\])(.*)",
      "matchPosition": 2,
      "color": "#7a5800",
      "font": "PPWriter-Semibold",
      "isRevealOnCursorRange": true
    },
    "interesting-left-delimiter": {
      "regex": "(\\>\\[!)(💡 Interesting: )(\\])(.*)",
      "matchPosition": 1,
      "color": "#777a5800",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "interesting-right-delimiter": {
      "regex": "(\\>\\[!)(💡 Interesting: )(\\])(.*)",
      "matchPosition": 3,
      "color": "#777a5800",
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true
    },
    "interesting-border": {
